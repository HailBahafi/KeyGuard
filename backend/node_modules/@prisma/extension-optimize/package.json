{
  "name": "@prisma/extension-optimize",
  "version": "2.0.1",
  "sideEffects": false,
  "description": "Prisma Optimize client extension for Prisma ORM",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "dependencies": {
    "@opentelemetry/api": "^1.8",
    "@opentelemetry/context-async-hooks": "^1.22",
    "@opentelemetry/instrumentation": "^0.49 || ^0.50 || ^0.51 || ^0.52.0 || ^0.53.0",
    "@opentelemetry/resources": "^1.22.0",
    "@opentelemetry/sdk-trace-base": "^1.22",
    "@opentelemetry/semantic-conventions": "1.22.0",
    "@prisma/debug": "6.9.0",
    "kleur": "4.1.5",
    "prisma-instrumentation-5-x": "npm:@prisma/instrumentation@5.19.1",
    "prisma-instrumentation-6-x": "npm:@prisma/instrumentation@6.9.0",
    "zod": "3.25.56"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@hono/node-server": "1.14.4",
    "@types/node-notifier": "8.0.5",
    "@types/readline-sync": "1.4.8",
    "eslint": "8.57.0",
    "hono": "4.7.11",
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "vitest": "3.2.3",
    "optimize-common": "0.0.0"
  },
  "peerDependencies": {
    "@prisma/client": "5.x || 6.x"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "check:exports": "attw . --pack",
    "lint": "eslint --fix .",
    "test": "vitest --run",
    "watch": "tsup --watch"
  }
}