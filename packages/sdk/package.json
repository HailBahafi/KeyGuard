{
  "name": "@keyguard/sdk",
  "version": "0.1.0",
  "description": "Secure TypeScript SDK for Device Binding to protect LLM API keys",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "security",
    "device-binding",
    "cryptography",
    "ecdsa",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^5.0.1",
    "idb-keyval": "^6.2.1"
  }
}
